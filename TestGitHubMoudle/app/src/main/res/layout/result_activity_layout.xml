<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ebf0ea">

    <com.example.administrator.testgithubmoudle.src.ui.TitleBar
        android:id="@+id/result_title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/result_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/result_button"
        android:textSize="20sp"
        android:textColor="@android:color/white"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="100dp"
        android:background="@drawable/circile_button" />

    <Button
        android:id="@+id/result_button_complete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/commit_button"
        android:textSize="20sp"
        android:textColor="@android:color/white"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="30dp"
        android:background="@drawable/circile_button"
        android:layout_marginLeft="200dp"/>

    <ListView
        android:id="@+id/result_list_view"
        android:layout_below="@id/result_title_bar"
        android:layout_above="@+id/result_button"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</RelativeLayout>